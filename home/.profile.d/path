# Coretuils
if [ `brew list | grep coreutils` ]; then
    export PATH=`brew --prefix coreutils`:$PATH
fi

# Postgres
export PATH=/opt/homebrew-cask/Caskroom/postgres/latest/Postgres.app/Contents/MacOS/bin:$PATH

# Local stuff
export PATH=~/bin:/usr/local/bin:/usr/local/sbin:/usr/local/heroku/bin:$PATH

# NPM
export PATH=$PATH:/usr/local/share/npm/bin

# Ruby/homebrew
export PATH=$PATH:/usr/local/opt/ruby193/bin

# pipsi - https://github.com/mitsuhiko/pipsi
export PATH=~/.local/bin:$PATH